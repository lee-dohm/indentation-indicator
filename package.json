{
  "name": "indentation-indicator",
  "author": "Lee Dohm <lee@lee-dohm.com>",
  "main": "./lib/main",
  "version": "0.6.0",
  "description": "Adds an indicator to the status bar that shows the indentation width and type of the active editor",
  "keywords": [
    "indentation",
    "indicator",
    "status-bar"
  ],
  "repository": "https://github.com/lee-dohm/indentation-indicator",
  "license": "MIT",
  "engines": {
    "atom": ">=1.8.0 <2.0.0"
  },
  "dependencies": {
    "etch": "^0.6.0"
  },
  "devDependencies": {
    "atom-mocha-test-runner": "0.2.1",
    "chai": "^3.5.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "atomTestRunner": "./spec/runner",
  "configSchema": {
    "spaceAfterColon": {
      "type": "boolean",
      "default": false
    },
    "indicatorPosition": {
      "type": "string",
      "default": "left",
      "enum": [
        "left",
        "right"
      ]
    }
  }
}
